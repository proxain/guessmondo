<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#000000" />
        </shape>
    </item>
    <item android:top="3dp" android:right="3dp" android:bottom="3dp"
          android:left="1dp">
        <shape android:shape="rectangle">
            <gradient
                    android:startColor="#EE931708"
                    android:centerColor="#F0AE2E1F"
                    android:endColor="#EE931708"
                    android:angle="90" />
        </shape>
    </item>
</layer-list>