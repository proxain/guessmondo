<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#000000" />
        </shape>
    </item>
    <item android:top="1dp" android:right="1dp" android:bottom="2dp"
          android:left="2dp">
        <shape android:shape="rectangle">
            <gradient
                    android:startColor="#6DB316"
                    android:centerColor="#296E15"
                    android:endColor="#6DB316"
                    android:angle="0" />
        </shape>
    </item>
</layer-list>


